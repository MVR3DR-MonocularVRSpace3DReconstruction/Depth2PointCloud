   "source": [
    "import os\n",
    "from PIL import Image\n",
    "\n",
    "for file in os.listdir():\n",
    "    filename, extension  = os.path.splitext(file)\n",
    "    if extension == \".pgm\":\n",
    "        new_file = \"{}.png\".format(filename)\n",
    "        with Image.open(file) as im:\n",
    "            im.save(new_file)"
   ]
  }
